

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model WorkshopUser {
  id        String   @id @default(uuid())
  email     String   
  name      String
  phone     String 
  stripeId  String?  // Stripe customer ID (optional)
  createdAt DateTime @default(now())
}